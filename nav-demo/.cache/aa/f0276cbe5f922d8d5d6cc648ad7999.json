  <div class=\"link\">${removeX(node.url)}</div>\r\n                <div class=\"close\"><svg class=\"icon\" >\r\n                <use xlink:href=\"#icon-close\"></use>\r\n            </svg></div>\r\n            </div>\r\n        </li> `).insertBefore(last)\r\n        $li.on('click', () => {\r\n            window.open(node.url)\r\n        })\r\n        $li.on('click', '.close', (e) => {\r\n            e.stopPropagation()\r\n            map.splice(index, 1)\r\n            render()\r\n\r\n        })\r\n    })\r\n}\r\nrender()\r\n\r\n\r\n\r\n$(\".addButton\").on(\"click\", () => {\r\n    let $siteList = $(\".siteList\")\r\n    console.log($siteList)\r\n    let url = window.prompt(\"请输入要新增的网址:\")\r\n    if (url === '') { return }\r\n    if (url.indexOf('http') !== 0) {\r\n        url = 'https://' + url\r\n    }\r\n    let domain = url.split(\"/\")\r\n    map.push({ logo: url[0], url: url })\r\n    render()\r\n})\r\n\r\nwindow.onbeforeunload = () => {\r\n    const string = JSON.stringify(map)\r\n    localStorage.setItem(\"list\", string)\r\n\r\n}"},"lineCount":null}},"error":null,"hash":"b9b377a80f9ca52d17741d30380709e1","cacheData":{"env":{}}}