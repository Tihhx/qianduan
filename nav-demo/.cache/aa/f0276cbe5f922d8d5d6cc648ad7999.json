  { logo: 'B', url: \"https://www.bilibili.com/\" },\r\n]\r\n\r\nconsole.log(map)\r\nconst removeX = (url) => {\r\n    return url.replace(\"https://\", '').replace(\"http://\", '').replace(\"www.\", '').replace(/\\/.*/, '')\r\n}\r\n\r\nfunction render() {\r\n    let $siteList = $(\".siteList\")\r\n    $siteList.find(\"li:not(.last)\").remove()\r\n    map.forEach((node, index) => {\r\n        if (node.url === '') { return }\r\n        let last = $(\".last\")\r\n        const $li = $(`<li>\r\n            <div class=\"site\">\r\n                <div class=\"logo\">${node.logo}</div>\r\n                <div class=\"link\">${removeX(node.url)}</div>\r\n                <div class=\"close\"><svg class=\"icon\" >\r\n                <use xlink:href=\"#icon-close\"></use>\r\n            </svg></div>\r\n            </div>\r\n        </li> `).insertBefore(last)\r\n        $li.on('click', () => {\r\n            window.open(node.url)\r\n        })\r\n        $li.on('click', '.close', (e) => {\r\n            e.stopPropagation()\r\n            map.splice(index, 1)\r\n            render()\r\n\r\n        })\r\n    })\r\n}\r\nrender()\r\n\r\n\r\n\r\n$(\".addButton\").on(\"click\", () => {\r\n    let $siteList = $(\".siteList\")\r\n    console.log($siteList)\r\n    let url = window.prompt(\"请输入要新增的网址:\")\r\n    if (url === '') { return }\r\n    if (url.indexOf('http') !== 0) {\r\n        url = 'https://' + url\r\n    }\r\n    let logo = removeX(url)[0].toUpperCase()\r\n    map.push({ logo: logo, url: url })\r\n    render()\r\n})\r\n\r\nwindow.onbeforeunload = () => {\r\n    const string = JSON.stringify(map)\r\n    localStorage.setItem(\"list\", string)\r\n\r\n}\r\n\r\n$(document).on(\"keypress\", (e) => {\r\n    let key = e.key\r\n    for (let i = 0; i < map.length; i++) {\r\n        if (key.toUpperCase() === map[i].logo.toUpperCase()) {\r\n            window.open(map[i].url)\r\n        }\r\n    }\r\n\r\n\r\n})"},"lineCount":null}},"error":null,"hash":"aea80be413f45c8555ad9e82d4c6f4ee","cacheData":{"env":{}}}